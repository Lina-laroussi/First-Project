<table>

<tr>
  <td> NSC</td>
  <td> name </td>
  <td> surname </td>
  <td> email</td>
  <td> action </td>
</tr>

{% for student in students %}
<tr>
    <td>{{student.nsc}}</td>
    <td>{{student.name}}</td>
    <td>{{student.surname}}</td>
    <td>{{student.email}}</td>
    <td> <a href="{{path('remove', {'nsc':student.nsc})}}"/>remove</td>
    <td> <a href="{{path('detail', {'nsc':student.nsc})}}"/>detail</td>
</tr>
{%endfor%}
</table>



